{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MongoDbOptions": {
    "ConnectionString": "mongodb://localhost:27017/",
    "DatabaseName": "Dance"
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "GroupId": "Store",
    "AutoOffsetReset": "Earliest",
    "EnableAutoCommit": true
  }
}